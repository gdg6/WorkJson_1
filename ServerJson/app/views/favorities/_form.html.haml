= form_for @favority do |f|
  - if @favority.errors.any?
    #error_explanation
      %h2= "#{pluralize(@favority.errors.count, "error")} prohibited this favority from being saved:"
      %ul
        - @favority.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :event
    = f.number_field :event_id
  .field
    = f.label :user
    = f.number_field :user_id
  .actions
    = f.submit 'Save'
